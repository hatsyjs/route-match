{
  "name": "@hatsy/route-match",
  "version": "1.1.0",
  "description": "Route matching library",
  "keywords": [
    "route",
    "route-match",
    "typescript"
  ],
  "homepage": "https://github.com/hatsyjs/route-match",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:hatsyjs/route-match.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hatsyjs/route-match/issues"
  },
  "main": "dist/route-match.js",
  "module": "dist/route-match.mjs",
  "exports": {
    ".": {
      "import": "./dist/route-match.mjs",
      "require": "./dist/route-match.js"
    }
  },
  "dependencies": {
    "@proc7ts/primitives": "^1.2.0"
  },
  "devDependencies": {
    "@proc7ts/eslint-config": "^2.0.4",
    "@proc7ts/rollup-helpers": "^1.0.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "codecov": "^3.7.2",
    "eslint": "^7.9.0",
    "eslint-plugin-jest": "^24.0.1",
    "gh-pages": "^3.1.0",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "rollup": "^2.27.1",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "run-z": "^1.1.1",
    "shx": "^0.3.2",
    "ts-jest": "^26.3.0",
    "tslib": "^2.0.1",
    "typedoc": "^0.19.0",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup --config ./rollup.config.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf d.ts dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+doc,+lint,+test"
  }
}
