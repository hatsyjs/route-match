{
  "name": "@hatsy/route-match",
  "version": "1.2.1",
  "description": "Route matching library",
  "keywords": [
    "route",
    "route-match",
    "typescript"
  ],
  "homepage": "https://github.com/hatsyjs/route-match",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:hatsyjs/route-match.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hatsyjs/route-match/issues"
  },
  "type": "module",
  "main": "./dist/route-match.cjs",
  "module": "./dist/route-match.js",
  "types": "./index.d.ts",
  "exports": {
    "import": "./dist/route-match.js",
    "require": "./dist/route-match.cjs"
  },
  "dependencies": {
    "@frontmeans/httongue": "^2.2.0",
    "@proc7ts/primitives": "^1.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@run-z/eslint-config": "^1.2.1",
    "@run-z/rollup-helpers": "^1.0.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "codecov": "^3.8.1",
    "eslint": "^7.19.0",
    "eslint-plugin-jest": "^24.1.3",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "rollup": "^2.38.5",
    "rollup-plugin-flat-dts": "^1.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "run-z": "^1.6.2",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.24",
    "typescript": "^4.1.5"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup --config rollup.config.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf index.d.ts dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+doc,+lint,+test"
  }
}
